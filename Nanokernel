						<!-- firstHeading -->
			<h1 id="firstHeading" class="firstHeading">Exokernel</h1>
			<!-- /firstHeading -->
			<!-- bodyContent -->
			<div id="bodyContent">
								<!-- tagline -->
				<div id="siteSub">From OSDev Wiki</div>
				<!-- /tagline -->
								<!-- subtitle -->
				<div id="contentSub">(Redirected from <a href="http://wiki.osdev.org/index.php?title=Nanokernel&amp;redirect=no" title="Nanokernel">Nanokernel</a>)</div>
				<!-- /subtitle -->
																<!-- jumpto -->
				<div id="jump-to-nav">
					Jump to: <a href="Nanokernel#mw-head">navigation</a>,
