						<!-- firstHeading -->
			<h1 id="firstHeading" class="firstHeading">ASCII</h1>
			<!-- /firstHeading -->
			<!-- bodyContent -->
			<div id="bodyContent">
								<!-- tagline -->
				<div id="siteSub">From OSDev Wiki</div>
				<!-- /tagline -->
