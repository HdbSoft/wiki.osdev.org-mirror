	udi_intr_event_cb_t <span class="sy0">*</span>intr_event_cb<span class="sy0">,</span>
	udi_ubit8_t flags<span class="br0">&#41;</span><span class="sy0">;</span>
&#160;
<span class="coMULTI">/* Provided by bus drivers; called when by the child driver to signal that it can handle more IRQs. */</span>
